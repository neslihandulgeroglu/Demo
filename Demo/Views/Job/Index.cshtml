@using EntityLayer.Concrete
@model List<Job>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_UILayout.cshtml";
}



<table class="table table-bordered">
    <tr>
        <th>Id</th>
        <th>Meslek Adı</th>
        <th>Sil</th>
        <th>Güncelle</th>
        <th>Detaylar</th>
    </tr>
    @foreach(var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            
            <td><a href="/Job/DeleteJob/@item.Id"class="btn btn-outline-danger">Sil</a></td>
            <td><a href="/Job/UpdateJob/@item.Id"class="btn btn-outline-success">Güncelle</a></td>
            <td><a href="/Job/DetailsJob/@item.Id"class="btn btn-outline-success">Detaylar</a></td>
           
        </tr>

    }
</table>



